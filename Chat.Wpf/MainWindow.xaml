<Window x:Class="Chat.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chat.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">

        <Button DockPanel.Dock="Top" Content="Launch a browser tab" Command="{Binding LaunchCommand}"/>

        <TextBox DockPanel.Dock="Bottom" Text="{Binding Input}"/>

        <TextBlock Background="Black" Foreground="White" Text="{Binding Output}"/>
        
    </DockPanel>
</Window>
